   -
     name : DisplayStep
     internal_properties : CAAAAAgAAABmkQLUreID8cjYV2GmJHu+eQYeSvaxPWY6nJTBsQqKm4zJazoJdBDpouoWGC5YqW6qkgbDFoiVszKO/YYcDpQhWRqIjAjIP85gaPJTW6DvlNU6qrzMgS7Nq9jjw55D0/o6iSqaU5RYk4eyTW63V2oXMHaMfYu8Z/znrHv3fKwcEWGDO/l6VbyZR2TGTkNHckvpkKtC3fY=
     procedure_id : 1760453209986083998
     type_code : 14
     code : |1-
      // Summary: Displays the explanation of step N
      // Caution: this procedure is used as a help for configuring the tracer
      // Syntax:
      //DisplayStep ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE DisplayStep()
      
      sExplanations is string
      sStepTitle is string
      sCommunicationType is string
      
      SWITCH gnCurrentStep
      	// "Normal" communication
      	CASE 0
      		sCommunicationType = <§$0002§>
      		sStepTitle = <§$0003§>
      		sExplanations = <§$0004§>
      		
      		
      	// Step 1: Enter the name of the Client application
      	CASE 1
      		sCommunicationType = <§$0005§>
      		sStepTitle = <§$0006§>
      		sExplanations = <§$0007§>
      		
      		
      	// Step 2: Enter the information of the Server application
      	CASE 2
      		sCommunicationType = <§$0005§>
      		sStepTitle = <§$0008§>
      		sExplanations = <§$0009§>
      		
      		
      	// Step 3: Enter the information in the client application
      	CASE 3
      		sCommunicationType = <§$0005§>
      		sStepTitle = <§$000a§>	
      		sExplanations = <§$000b§>
      		
      		
      	// Step 4: Start the Tracer
      	CASE 4
      		sCommunicationType = <§$0005§>
      		sStepTitle = <§$000c§>	
      		sExplanations = <§$000d§>
      		
      END
      
      // Displays the explanations for the current step
      STC_Explanations = sExplanations
      STC_StepTitle = sStepTitle
      STC_TitleCommunicationType = sCommunicationType
      
      // Displays (highlights) the associated controls
      DisplayControl()
     type : 458752
   -
     name : DisplayControl
     procedure_id : 1760453209986149534
     type_code : 14
     code : |1-
      // Summary: Displays, highlights or hides some controls
      // Caution: this procedure is used as a help for configuring the tracer
      // Syntax:
      //DisplayControl ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE DisplayControl()
      
      nBckgrdColorHighlight is string = RGB(102,153,255)
      
      BTN_PreviousStep..State = Active
      BTN_NextStep..State = Active
      
      SWITCH gnCurrentStep
      	// "Normal" communication
      	CASE 0
      		GR_NormalComm..Visible = True
      		GR_ClientToTracer..Visible = False
      		GR_ServerToTracer..Visible = False
      		GR_TracerToClient..Visible = False
      		GR_TracerToServer..Visible = False
      		
      		GR_ClientControl..BackgroundColor = iWhite
      		GR_ServerControl..BackgroundColor = iWhite
      		GR_TracerControl..BackgroundColor = iWhite
      		BTN_PreviousStep..State = Grayed
      		
      	// Step 1: Enter the name of the Client application
      	CASE 1
      		GR_NormalComm..Visible = False
      		GR_ClientToTracer..Visible = True
      		GR_ServerToTracer..Visible = True
      		GR_TracerToClient..Visible = True
      		GR_TracerToServer..Visible = True
      		
      		GR_ClientControl..BackgroundColor = nBckgrdColorHighlight
      		GR_ServerControl..BackgroundColor = iWhite
      		GR_TracerControl..BackgroundColor = iWhite
      		
      	// Step 2: Enter the information of the Server application
      	CASE 2
      		GR_NormalComm..Visible = False
      		GR_ClientToTracer..Visible = True
      		GR_ServerToTracer..Visible = True
      		GR_TracerToClient..Visible = True
      		GR_TracerToServer..Visible = True
      		
      		GR_ClientControl..BackgroundColor = iWhite
      		GR_ServerControl..BackgroundColor = nBckgrdColorHighlight
      		GR_TracerControl..BackgroundColor = iWhite
      		
      		
      	// Step 3: Enter the information in the client application
      	CASE 3
      		GR_NormalComm..Visible = False
      		GR_ClientToTracer..Visible = True
      		GR_ServerToTracer..Visible = True
      		GR_TracerToClient..Visible = True
      		GR_TracerToServer..Visible = True
      		
      		GR_ClientControl..BackgroundColor = iWhite
      		GR_ServerControl..BackgroundColor = iWhite
      		GR_TracerControl..BackgroundColor = nBckgrdColorHighlight
      		
      	// Step 4: Start the tracer
      	CASE 4
      		GR_NormalComm..Visible = False
      		GR_ClientToTracer..Visible = True
      		GR_ServerToTracer..Visible = True
      		GR_TracerToClient..Visible = True
      		GR_TracerToServer..Visible = True
      		
      		GR_ClientControl..BackgroundColor = iWhite
      		GR_ServerControl..BackgroundColor = iWhite
      		GR_TracerControl..BackgroundColor = iWhite
      		
      		BTN_NextStep..State = Grayed
      		
      	OTHER CASE
      		
      END
