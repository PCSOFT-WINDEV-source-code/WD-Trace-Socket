     name : TestValue
     procedure_id : 1760453209986346142
     type_code : 14
     code : |1-
      // Summary: Checks the edit control
      // Syntax:
      //TestValue ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE TestValue()
      
      // Tracer address
      IF EDT_LOCALIP = "" THEN
      	Error(<§$000e§>)
      	ReturnToCapture(EDT_LOCALIP)
      END
      
      // Server Address
      IF EDT_REALIP = "" THEN
      	Error(<§$000f§>)
      	ReturnToCapture(EDT_REALIP)
      END
      
      // Tracer port
      IF EDT_LOCALPORT = "" THEN
      	Error(<§$0010§>)
      	ReturnToCapture(EDT_LOCALPORT)
      END
      
      // Server port
      IF EDT_REALPORT = "" THEN
      	Error(<§$0011§>)
      	ReturnToCapture(EDT_REALPORT)
      END
      
      // Customer name
      IF EDT_CLIENTNAME = "" THEN
      	Error(<§$0012§>)
      	ReturnToCapture(EDT_CLIENTNAME)
      END
      
      // Name of server
      IF EDT_SERVERNAME = "" THEN
      	Error(<§$0013§>)
      	ReturnToCapture(EDT_LOCALIP)
      END
