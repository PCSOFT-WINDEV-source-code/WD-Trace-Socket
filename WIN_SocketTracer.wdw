  -
    name : BTN_Start
    identifier : 0x286e62de0795ac9e
    internal_properties : 
    properties :
     x : 406
     y : 436
     width : 80
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         // Checks whether values are entered
         TestValue()
         
         // Check on the ports
         IF EDT_LOCALPORT = 0 THEN
         	Error(<§$0014§>)
         	ReturnToCapture(EDT_LOCALPORT)
         END
         // Check on the ports
         IF EDT_REALPORT = 0 THEN
         	Error(<§$0014§>)
         	ReturnToCapture(EDT_REALPORT)
         END
         
         // Start the intermediate trace server
         IF NOT gclSnifSocket:ServerStart(EDT_CLIENTNAME, EDT_LOCALIP, EDT_LOCALPORT, EDT_SERVERNAME, EDT_REALIP, EDT_REALPORT) THEN
         	Error(<§$0000§>, gclSnifSocket:ErrorMessage())
         ELSE
         	// Connection OK
         	// Grays the controls that cannot be used during the connection
         	GR_ControlToDisable..State = Grayed
         	
         	// Positions the diagram on the final step
         	gnCurrentStep = 4
         	DisplayStep()
         	
         END
        type : 18
      -
        type : 161
        enabled : false
  -
    name : BTN_Stop
    identifier : 0x286e62de0796ac9e
    internal_properties : 
    properties :
     x : 492
     y : 436
     width : 80
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         // Stops the intermediate trace socket server
         IF NOT gclSnifSocket:ServerStop() THEN
         	// Pb stopping the server
         	Error(gclSnifSocket:ErrorMessage())
         ELSE
         	// Server stopped
         	MySelf..State = Grayed
         	GR_ControlToDisable..State = Active
         END
        type : 18
