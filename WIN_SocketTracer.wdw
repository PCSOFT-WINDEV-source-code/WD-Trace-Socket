   -
     name : PreviousStep
     procedure_id : 1760453209986215070
     type_code : 14
     code : |1-
      // Summary: Displays the previous step
      // Caution: this procedure is used as a help for configuring the tracer
      // Syntax:
      //PreviousStep ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE PreviousStep()
      
      // Step 0 is the first one
      IF gnCurrentStep = 0 THEN RETURN
      
      // Decrements the current step
      gnCurrentStep--
      
      // Prepares the animation
      AnimationPrepare(MyWindow)
      
      // Displays the step
      DisplayStep()
      
      // Plays the animation
      AnimationPlay(animFadeIn)
     type : 458752
   -
     name : NextStep
     procedure_id : 1760453209986280606
     type_code : 14
     code : |1-
      // Summary: Displays the next step
      // Caution: this procedure is used as a help for configuring the tracer
      // Syntax:
      //NextStep ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE NextStep()
      
      // Step 4 is the last one
      IF gnCurrentStep = 4 THEN RETURN
      
      // Increments the current step
      gnCurrentStep++
      
      // Prepares the animation
      AnimationPrepare(MyWindow)
      
      // Displays the step
      DisplayStep()
      
      // Plays the animation
      AnimationPlay(animFadeIn)
